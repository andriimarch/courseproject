							"                                status: { type: \"string\" },",
							"                                type: { type: \"string\" },",
							"                                orderindex: { type: \"number\" },",
							"                                color: { type: \"string\" }",
							"                            },",
							"                            required: [\"id\", \"status\", \"type\", \"orderindex\", \"color\"]",
							"                        }",
							"                    },",
							"                    multiple_assignees: { type: \"boolean\" },",
							"                    features: {",
							"                        type: \"object\",",
							"                        properties: {",
							"                            due_dates: {",
							"                                type: \"object\",",
							"                                properties: {",
							"                                    enabled: { type: \"boolean\" },",
